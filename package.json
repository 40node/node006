{
  "name": "library-management",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "build": "npm run migrate && npm run seed",
    "clean": "sequelize db:seed:undo:all",
    "retest": "npm run clean && npm run build && npm test",
    "test-with-coverage": "istanbul cover node_modules/.bin/jasmine --captureExceptions && cat ./coverage/lcov.info | codacy-coverage && rm -rf ./coverage",
    "test": "jasmine"
  },
  "engines": {
    "node": "11.10.1"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "ejs": "~2.5.7",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "jasmine": "^3.3.0",
    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.7.1",
    "rewire": "^4.0.1",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^4.1.1",
    "serve-favicon": "~2.4.5",
    "supertest": "^3.3.0",
    "supertest-session": "^3.3.0"
  },
  "devDependencies": {
    "codacy-coverage": "^3.4.0",
    "istanbul": "^0.4.5",
    "sqlite3": "^4.0.6",
    "textlint": "^11.2.3",
    "textlint-plugin-review": "^0.3.3",
    "textlint-rule-preset-ja-spacing": "^2.0.1",
    "textlint-rule-preset-ja-technical-writing": "^3.1.0",
    "textlint-rule-preset-jtf-style": "^2.3.3",
    "textlint-rule-prh": "^5.2.0",
    "textlint-rule-spellcheck-tech-word": "^5.0.0"
  }
}