{
  "name": "library-management",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "build": "npm run migrate && npm run seed",
    "clean": "sequelize db:seed:undo:all",
    "retest": "npm run clean && npm run build && npm test",
    "test-with-coverage": "istanbul cover node_modules/.bin/jasmine --captureExceptions && cat ./coverage/lcov.info | codacy-coverage && rm -rf ./coverage",
    "test": "jasmine"
  },
  "engines": {
    "node": "11.6.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "jasmine": "^3.3.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.7.1",
    "rewire": "^4.0.1",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^4.1.1",
    "serve-favicon": "~2.4.5",
    "supertest": "^3.3.0",
    "supertest-session": "^3.3.0"
  },
  "devDependencies": {
    "codacy-coverage": "^3.3.0",
    "istanbul": "^0.4.5",
    "sqlite3": "^4.0.4"
  }
}